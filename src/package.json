{
	"name": "@beyond-js/routing",
	"version": "0.0.1",
	"title": "BeyondJS Routing",
	"description": "A versatile client-side routing package for Beyond.js, supporting both hash and pathname modes with seamless navigation and history management.",
	"license": "ISC",
	"template": "template/template.json",
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"platform": "web",
				"environment": "local",
				"ports": {
					"bundles": 1010
				}
			}
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.9",
		"@beyond-js/local": "~0.1.3",
		"socket.io-client": "^4.5.4"
	}
}
