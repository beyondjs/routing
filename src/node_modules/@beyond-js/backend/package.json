{
	"name": "@beyond-js/backend",
	"version": "0.1.9",
	"description": "This package is a key component of BeyondJS and must be installed as a dependency to use Bridges in your project",
	"license": "MIT",
	"libraries": {
		"imports": [
			"@beyond-js/kernel",
			"@beyond-js/local"
		]
	},
	"dependencies": {
		"@beyond-js/kernel": "~0.1.8",
		"socket.io": "^4.5.4",
		"socket.io-client": "^4.5.4"
	},
	"devDependencies": {
		"@beyond-js/local": "~0.1.3",
		"@types/node": "^20.4.0"
	},
	"static": {},
	"exports": {
		"./client": {
			"types": "./client/client.d.ts",
			"amd": "./client/client.amd.js",
			"sjs": "./client/client.sjs.js",
			"browser": "./client/client.browser.mjs",
			"module": "./client/client.browser.mjs",
			"node": {
				"require": "./client/client.cjs.js",
				"import": "./client/client.mjs"
			}
		},
		"./listen": {
			"types": "./listen/listen.d.ts",
			"node": {
				"require": "./listen/listen.cjs.js",
				"import": "./listen/listen.mjs"
			}
		},
		"./server": {
			"types": "./server/server.d.ts",
			"node": {
				"require": "./server/server.cjs.js",
				"import": "./server/server.mjs"
			}
		},
		"./config": {
			"amd": "./config/config.amd.js",
			"sjs": "./config/config.sjs.js",
			"browser": "./config/config.browser.mjs",
			"module": "./config/config.browser.mjs",
			"node": {
				"import": "./config/config.mjs",
				"require": "./config/config.cjs.js"
			}
		}
	},
	"uimport": [
		"socket.io-client",
		"http",
		"socket.io"
	]
}
